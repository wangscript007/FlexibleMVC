@{
    string adminLTE = LessConfig.AdminLTE_Path;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <!-- Pace -->
    <link href="@Url.Content(adminLTE + "plugins/pace-master/themes/blue/pace-theme-flash.css")" rel="stylesheet" type="text/css">
    <script src="@Url.Content(adminLTE + "plugins/pace-master/pace.min.js")" type="text/javascript"></script>

    <!-- Bootstrap 3.3.6 -->
    <link href="@Url.Content(adminLTE + "bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css">
    <!-- Font Awesome -->
    <link href="@Url.Content(adminLTE + "dist/css/font-awesome.min.css")" rel="stylesheet" type="text/css">
    <!-- Ionicons -->
    <link href="@Url.Content(adminLTE + "dist/css/ionicons.min.css")" rel="stylesheet" type="text/css">
    <!-- jvectormap -->
    <link href="@Url.Content(adminLTE + "plugins/jvectormap/jquery-jvectormap-1.2.2.css")" rel="stylesheet" type="text/css">
    <!-- Theme style -->
    <link href="@Url.Content(adminLTE + "dist/css/AdminLTE.min.css")" rel="stylesheet" type="text/css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="@Url.Content(adminLTE + "dist/css/skins/all-skins.min.css")" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="@Url.Content(adminLTE + "html5shiv/html5shiv.min.js")" type="text/javascript"></script>
    <script src="@Url.Content(adminLTE + "html5shiv/respond.min.js")" type="text/javascript"></script>

    <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Content Header (Page header) -->
    @*<section class="content-header">
            <h1>
                Dashboard
                <small>Version 2.0</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Dashboard</li>
            </ol>
        </section>*@

    <!-- Main content -->
    <section class="content">
        @RenderBody()
    </section>
    <!-- /.content -->
    <!-- jQuery 2.2.3 -->
    <script src="@Url.Content(adminLTE + "plugins/jQuery/jquery-2.2.3.min.js")" type="text/javascript"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="@Url.Content(adminLTE + "bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <!-- FastClick -->
    <script src="@Url.Content(adminLTE + "plugins/fastclick/fastclick.js")" type="text/javascript"></script>
    <!-- AdminLTE App -->
    <script src="@Url.Content(adminLTE + "dist/js/app.min.js")" type="text/javascript"></script>
    <!-- Sparkline -->
    <script src="@Url.Content(adminLTE + "plugins/sparkline/jquery.sparkline.min.js")" type="text/javascript"></script>
    <!-- jvectormap -->
    <script src="@Url.Content(adminLTE + "plugins/jvectormap/jquery-jvectormap-1.2.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content(adminLTE + "plugins/jvectormap/jquery-jvectormap-world-mill-en.js")" type="text/javascript"></script>

    <!-- SlimScroll 1.3.0 -->
    <script src="@Url.Content(adminLTE + "plugins/slimScroll/jquery.slimscroll.min.js")" type="text/javascript"></script>

    <!-- ChartJS 1.0.1 -->
    <script src="@Url.Content(adminLTE + "plugins/chartjs/Chart.min.js")" type="text/javascript"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @*<script src="@Url.Content(adminLTE + "dist/js/pages/dashboard2.js")" type="text/javascript"></script>*@
    <!-- AdminLTE for demo purposes -->
    <script src="@Url.Content(adminLTE + "dist/js/demo.js")" type="text/javascript"></script>

    @{
        foreach (var js in ViewBag.Scripts)
        {
            <script src="@Url.Content(adminLTE + js)" type="text/javascript"></script>
        }
    }

    <script src="@Url.Content("~/Scripts/admin/util.js")" type="text/javascript"></script>

    <!-- page js -->
    <script src="@Url.Content("~/Scripts/" + ViewBag.PageScript)" type="text/javascript"></script>
</body>
</html>